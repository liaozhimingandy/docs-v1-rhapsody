/* EDITED */
       function reorderSections(commentSection, labelsSection, childPagesSection) {
		if (commentSection && labelsSection) {
			labelsSection.parentNode.insertBefore(commentSection, labelsSection);
		}
		if (childPagesSection && labelsSection) {
			childPagesSection.parentNode.insertBefore(childPagesSection, labels);
		}
	}
/* EDITED */

	makeCommentBar();


	function makeCommentBar() {
		var buttonHTML = '<span id="primary" class="yui-primary-button yui-push-button"><span class="first-child"><button type="button" tabindex="0" >Add a Comment</button></span></span>';

		var commentTop = document.getElementById('add-comment-top');
		var commentLink = document.getElementById('add-comment-bottom');
		var commentSection = document.getElementById('comments-section');
		if (commentSection) {
			if (commentTop && jQuery('[name=confirm]').length > 0) {
				addingComment(commentTop, buttonHTML);
			} else if (commentLink) {
				addCommentBar(commentLink, commentSection, buttonHTML);
			}
		}
	}



	function addingComment(commentTop, buttonHTML) {
		var confirmButton = jQuery('[name=confirm]')[0];
		var cancelButton = jQuery('[name=cancel]')[0];

		inputAsButton(secondaryHTML, cancelButton);
		inputAsButton(buttonHTML, confirmButton);
		/*put int the buttons bg */
		titleComments.parentNode.className += " buttons";

	}
	/**
	 * replaces a button with an input[type=button] so that it can be styled as  a new look button
	 *@param InnerHTML: the old html form of a button comprising 3 layers 
	 * of spans with a button in the middle
	 * @param originalButton: the input[type=button] you want to replace the button in.
	 **/
	function inputAsButton(innerHTML, originalButton) {
		var container = originalButton.parentNode;
		var outy = document.createElement('span');
		outy.innerHTML = innerHTML;

		/* re-hackage the button html*/
		oldButtonNode = outy.firstChild.firstChild;
		oldButtonNode.replaceChild(originalButton, oldButtonNode.firstChild);

		container.insertBefore(outy, container.firstChild);
	}



	function addCommentBar(commentLink, commentSection, buttonHTML) {

		var commentNumber = document.getElementById('comment-show-hide');

		if (commentNumber) {
			commentSection.appendChild(commentLink.cloneNode(true));
			existingComments(commentLink, commentNumber);

			var extraCommentLink = document.getElementById('add-comment-top').parentNode;
			removeElement(extraCommentLink);
		} else {
			noComments(commentLink, commentSection);
		}
		commentLink.innerHTML = buttonHTML;

		var commentMenuButton = commentLink.cloneNode(true);
		commentMenuButton.innerHTML = secondaryHTML;
		attachMenuButton(commentMenuButton);

	}


	function existingComments(commentLink, commentNumber) {
		var buttons = commentNumber.parentNode.parentNode;

		buttons.className += ' buttons';
		buttons.insertBefore(commentLink, buttons.firstChild);
	}


	function noComments(commentLink, commentSection) {
		var buttons = document.createElement('div');
		var commentLinkParent = commentLink.parentNode;

		buttons.className = 'buttons';

		commentLinkParent.replaceChild(buttons, commentLink);
		buttons.appendChild(commentLink);

		//and move it up 
		commentSection.insertBefore(commentLinkParent, commentSection.firstChild);
	}



	function removeElement(element) {
		element.parentNode.removeChild(element);
	}

	/** attaches button to the top menu**/
	function attachMenuButton(button) {
		var addLink = document.getElementById('add-menu-link');

		/* insert a new comments link */
		if (addLink) {
			var menuBarUL = addLink.parentNode.parentNode;
			var commentButconLi = document.createElement('li');
			commentButconLi.className = 'ajs-button normal';

			commentButconLi.appendChild(button);
			menuBarUL.insertBefore(commentButconLi, menuBarUL.firstChild);
		}
	}
	moveSurvey(commentSection);


	function moveSurvey(commentSection) {
		var survey = document.getElementById('theme-footer');
		if (survey) {
			var parent = commentSection.parentNode;
			parent.insertBefore(survey, commentSection);
		}
	}
	
		} catch(e) {}
	});

</script>





